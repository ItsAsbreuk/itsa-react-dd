{
    "name": "itsa-react-dd",
    "version": "16.0.0",
    "description": "Drag and Drop for React without a wrapper",
    "author": [
        {
            "name": "Marco Asbreuk",
            "email": "marco@itsasbreuk.nl"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ItsAsbreuk/itsa-react-dd"
    },
    "bugs": {
        "url": "https://github.com/ItsAsbreuk/itsa-react-dd/issues"
    },
    "keywords": [
        "itsasbreuk",
        "itsa",
        "react",
        "drag",
        "drop",
        "drag and drop"
    ],
    "main": "./index.js",
    "engines": {
        "node": "*"
    },
    "directories": {
        "css": "./css",
        "lib": "./lib"
    },
    "peerDependencies": {
        "react": ">=15",
        "react-dom": ">=15"
    },
    "devDependencies": {
        "babel-core": "^6.7.6",
        "babel-eslint": "^6.0.2",
        "babel-loader": "^6.2.4",
        "babel-preset-es2015": "^6.6.0",
        "babel-preset-react": "^6.5.0",
        "chai": "^3.5.0",
        "eslint": "^4.0.0",
        "eslint-plugin-react": "^7.1.0",
        "istanbul": "^0.3.0",
        "jsdom": "^7.2.2",
        "jsdom-global": "^1.6.1",
        "jshint": "^2.5.5",
        "mocha": "^1.21.4",
        "node-sass": "^4.5.3",
        "react": "^15.6.1",
        "react-addons-test-utils": "^15.0.1",
        "react-dom": "^15.6.1",
        "sass-loader": "^3.1.2",
        "style-loader": "^0.13.0",
        "webpack": "^1.12.2",
        "yuidocjs": "^0.9.0"
    },
    "scripts": {
        "pretest": "eslint  ./**/*.jsx?",
        "test": "mocha 'tests/*.js' --compilers js:babel-core/register",
        "lint": "eslint ./**/*.jsx?",
        "build": "webpack && yuidoc ./lib -q -c ./yuidoc.json"
    },
    "maintainers": [
        {
            "name": "Marco Asbreuk",
            "email": "marco@itsasbreuk.nl"
        }
    ],
    "testling": {
        "harness": "mocha",
        "files": "tests/*.js",
        "browsers": [
            "ie/6..latest",
            "chrome/22..latest",
            "firefox/16..latest",
            "safari/latest",
            "opera/11.0..latest",
            "iphone/6",
            "ipad/6",
            "android-browser/latest"
        ]
    },
    "license": "BSD-3-Clause",
    "dependencies": {
        "itsa-dom": "^1.2.0",
        "itsa-event": "^1.1.10",
        "itsa-jsext": "^1.4.0",
        "itsa-utils": "^1.4.0"
    }
}
